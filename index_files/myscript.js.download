$(document).ready(function(){
	var audioElement = document.getElementById('musicTrigger');

	
	$(".main").slideUp();
	
	$("#jquery").click(function(){
		$(".main").slideToggle();
	});
	
	$("a").mouseover(function() {
		$(this).css("color", "#ffd86d");
	});
	$("a").mouseout(function() {
		$(this).css("color", 'white');
	});
	
	$(".me").click(function() {
		audioElement.play();
	});
});